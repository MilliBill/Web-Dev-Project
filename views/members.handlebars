<div>
  <h1>Members</h1>
  <p>Here is a list of our most involved members of the Kirby Club. If you want
    to see what events we have worked on click on our names</p>

</div>

<body>
  {{#if members.length}}
    <table>
      <thead>
        <tr>
          <th>Username</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>Joined Date</th>
          <th>Phone Number</th>
        </tr>
      </thead>
      <tbody>
        {{#each members}}
          <tr>
            <td>
              <button class="member-button" data-username="{{this.username}}">
                {{this.username}}
              </button>
            </td>
            <td>{{this.fname}}</td>
            <td>{{this.lname}}</td>
            <td>{{this.email}}</td>
            <td>{{this.joined_date}}</td>
            <td>{{this.phone_number}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  {{else}}
    <p>No members found.</p>
  {{/if}}
</body>